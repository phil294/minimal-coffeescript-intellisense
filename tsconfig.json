{
	"compilerOptions": {
		"target":"ESNext",
		"allowJs": true,
		"checkJs": true,
		"module": "commonjs",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"noEmit": true
	},
	"exclude": [
		"node_modules",
	],
}